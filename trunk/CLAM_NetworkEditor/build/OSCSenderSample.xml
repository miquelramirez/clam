<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<network id="CLAM">

  <processing id="OSCSender" type="OSCSender">
    <HostName>localhost</HostName>
    <Port>7000</Port>
    <Path>/Oscillator/Pitch</Path>
  </processing>

  <processing id="Oscillator" type="SimpleOscillator">
    <Frequency>440</Frequency>
    <Amplitude>1</Amplitude>
    <Phase>0</Phase>
    <SamplingRate>44100</SamplingRate>
  </processing>

  <processing id="OutControlSender_0" type="OutControlSender">
    <Min>0</Min>
    <Default>0</Default>
    <Max>10</Max>
    <Step>0.1</Step>
    <ControlRepresentation>Vertical Slider</ControlRepresentation>
  </processing>

  <processing id="Random Generator" type="Random">
    <Name>Random_Generator</Name>
    <DelayFactor>10</DelayFactor>
    <MinNote>36</MinNote>
    <MaxNote>98</MaxNote>
  </processing>

  <processing id="out" type="AudioOut">
    <Device>default:default</Device>
    <ChannelID>0</ChannelID>
    <FrameSize>512</FrameSize>
    <SampleRate>44100</SampleRate>
  </processing>

  <port_connection>
    <out>Oscillator.Audio Output</out>
    <in>out.Audio Input</in>
  </port_connection>

  <control_connection>
    <out>OutControlSender_0.out</out>
    <in>Random Generator.In delay factor</in>
  </control_connection>

  <control_connection>
    <out>Random Generator.Out frequency</out>
    <in>OSCSender.input</in>
  </control_connection>

</network>
